<%= form_for(path) do |f| %>
  <% if path.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(path.errors.count, "error") %> prohibited this path from being saved:</h2>

      <ul>
      <% path.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :degree_id %>
    <%= f.number_field :degree_id, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :start_quarter %>
    <%= f.text_field :start_quarter, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :course_load %>
    <%= f.number_field :course_load, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :in_class %>
    <%= f.number_field :in_class, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :online %>
    <%= f.number_field :online, class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit :Submit, class:"btn btn-success" %>
  </div>
<% end %>
