<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 panel panel-default">
        <div class="panel-body">
					<div class="margin-vertical">
			  <strong>Course Code:</strong>
			  <%= @course.course_code %>
			</div>
					<div class="margin-vertical">
			  <strong>Name:</strong>
			  <%= @course.name %>
			</div>
					<div class="margin-vertical">
			  <strong>Description:</strong>
			  <%= @course.description %>
			</div>
		        </div>

        <div class="panel-footer">
		<%= link_to 'Back', courses_path, class:"btn btn-xs btn-default margin-right color-white" %>
		<%= link_to 'Edit', edit_course_path(@course), class:"btn btn-xs btn-warning color-white" %>
	</div>
</div>
