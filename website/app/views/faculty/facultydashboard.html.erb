<div class="page-header">
  <h1>Faculty Dashboard</h1>
</div>

<div class="margin-vertical">
  <select name="student">
    <option disabled selected value> -- Select a Student -- </option>
    <% @users.each do |user| %>
      <% if user.student? %>
        <option value=<% user.id %>><%= user.first %> <%= user.last %></option>
      <% end %>
    <% end %>
  </ul>
</div>
